                            <i class="fa-solid fa-moon"></i>
                        </button>
                        <div class="brands d-flex justify-content-center align-items-center">
                            @if (User.Identity.IsAuthenticated)
                            {
                                <a asp-action="Profile" asp-controller="User"><i class="fa-solid fa-user"></i></a>
                            }
                            else
                            {
                            
                                <a asp-action="Login" asp-controller="Auth"><i class="fa-solid fa-user"></i></a>
                                
                            }
                            
                        </div>
                        <div class="brands d-flex justify-content-center align-items-center">
                            <a href="./pages/cart.html"><i class="fa-solid fa-bag-shopping"></i></a>
                            @* <div class="shopping-amount d-flex justify-content-center align-items-center">
                                <p id="cart-amount">0</p>
                            </div> *@
                        </div>                        

