<body>
    <div class="loading"></div>
    <section id="loading-pages" class="mobile" style="left: 100%;">
        <section class="mobile-logo py-3 pb-md-5">
            <div class="container-mobile">
                <div class="d-flex justify-content-between align-items-center">
                    <div class="img-container">
                        <img src="~/assets/images/kaffa_logo_dark_2x.png" alt="">
                    </div>
                    <button class="icon-x">
                        <i class="fa-solid fa-xmark"></i>
                    </button>
                </div>
            </div>
        </section>
        <section class="mobile-nav">
            <div class="container-mobile">
                <ul>
                    <li class="d-flex justify-content-between align-items-center">
                        <a class="active langdata" href="index.html">Home</a>
                        <button class="icons">
                            <i class="fa-solid fa-plus"></i>
                        </button>
                    </li>
                    <li class="d-flex justify-content-between align-items-center">
                        <a class="langdata" href="pages/faq.html">FAQ</a>
                        <button class="icons">
                            <i class="fa-solid fa-plus"></i>
                        </button>
                    </li>
                    <li class="d-flex justify-content-between align-items-center">
                        <a class="langdata" href="pages/products.html"> Products</a>
                        <button class="icons">
                            <i class="fa-solid fa-plus"></i>
                        </button>
                    </li>
                    <li class="d-flex justify-content-between align-items-center">
                        <a class="langdata" href="pages/blog.html">Blog</a>
                        <button class="icons">
                            <i class="fa-solid fa-plus"></i>
                        </button>
                    </li>
                    <li class="d-flex justify-content-between align-items-center">
                        <a class="langdata" href="pages/contacts.html">Contacts</a>
                        <button class="icons">
                            <i class="fa-solid fa-plus"></i>
                        </button>
                    </li>
                </ul>
            </div>
        </section>
        <section class="search-section">
            <div class="container-mobile">
                <div class="d-flex justify-content-between align-items-center py-3">
                    <div class="d-flex gap-4 align-items-center icons-left">
                        <div class="icons">
                            <a href="./pages/cart.html"><i class="fa-solid fa-bag-shopping"></i></a>
                        </div>
                        <div class="icons d-flex justify-content-center align-items-center">
                            <p id="cart-amount2">0</p>
                        </div>
                    </div>
                    <div class="icons">
                        <a asp-action="Login" asp-controller="auth"><i class="fa-solid fa-user"></i></a>
                    </div>
                </div>
                <div>
                    <form>
                        <input type="search" placeholder="Search">
                        <button><i class="fa-solid fa-magnifying-glass icon"></i></button>
                    </form>
                </div>
            </div>
        </section>
    </section>
    <header id="loading-pages" class="home">
        <div class="background"></div>
        <section class="header-logo py-3">
            <div class="container">
                <div class="dark-mode-con position-absolute d-block d-xl-none">
                    <button class="brands d-flex justify-content-center align-items-center dark-mode-btn">
                        <i class="fa-solid fa-moon"></i>
                    </button>
                </div>
                <div class="d-flex justify-content-center justify-content-xl-between align-items-center">
                    <div class="d-none d-xl-flex align-items-center">
                        <p class="follow-us langdata">Follow us</p>
                        <div class="d-flex gap-2">
                            <div class="brands d-flex justify-content-center align-items-center">
                                <i class="fa-brands fa-twitter"></i>
                            </div>
                            <div class="brands d-flex justify-content-center align-items-center">
                                <i class="fa-brands fa-facebook-f"></i>
                            </div>
                            <div class="brands d-flex justify-content-center align-items-center">
                                <i class="fa-brands fa-instagram"></i>
                            </div>
                            <div class="brands d-flex justify-content-center align-items-center">
                                <i class="fa-brands fa-youtube"></i>
                            </div>
                        </div>
                    </div>
                    <div class="img-container">
                        <img src="~/assets/images/kaffa_logo_light_2x.png" alt="">
                    </div>
                    <div class="d-none d-xl-flex gap-4 icons-right">
