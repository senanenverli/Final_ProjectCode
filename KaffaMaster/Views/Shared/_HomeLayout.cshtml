                        <li><a class="langdata" asp-action="Index" asp-controller="FAQ">FAQ <i class="fa-solid fa-chevron-left"></i></a></li>
                        <li><a class="langdata" asp-action="Index" asp-controller="Shop">Products <i class="fa-solid fa-chevron-left"></i></a></li>
                        <li><a class="langdata" asp-action="Index" asp-controller="Blog">Blog <i class="fa-solid fa-chevron-left"></i></a></li>
                        <li><a class="langdata" asp-action="Index" asp-controller="Contact">Contacts <i class="fa-solid fa-chevron-left"></i></a></li>
