                    <form asp-controller="Subscription" asp-action="Subscribe" class="mt-4" method="post">
                        <div>
                            <input asp-for="Email" placeholder="Your email ..." class="email" type="email">
                            <button class="langdata">Subscribe</button>
                        </div>
                        <div class="mt-4">
                            <input type="checkbox">
                            <label for="" class="langdata">I have read and agree to the terms & conditions</label>
                        </div>
                    </form> 
                </div>
            </div>
        </div>
    </footer>
    <script src="~/assets/js/index.js"></script>
</body>

